{
  "name": "@deepdish/ui",
  "version": "0.5.3",
  "description": "DeepDish UI components",
  "author": {
    "name": "ByteSlice, LLC",
    "url": "https://byteslice.co"
  },
  "exports": {
    "./config": {
      "import": "./dist/config.mjs",
      "types": "./dist/config.d.ts",
      "require": "./dist/config.js"
    },
    "./link": {
      "import": "./dist/elements/link.mjs",
      "types": "./dist/elements/link.d.ts",
      "require": "./dist/elements/link.js"
    },
    "./media": {
      "import": "./dist/elements/media.mjs",
      "types": "./dist/elements/media.d.ts",
      "require": "./dist/elements/media.js"
    },
    "./typography": {
      "import": "./dist/elements/typography.mjs",
      "types": "./dist/elements/typography.d.ts",
      "require": "./dist/elements/typography.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rehype-parse": "9.0.0",
    "rehype-sanitize": "6.0.0",
    "rehype-stringify": "10.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.0",
    "server-only": "0.0.1",
    "unified": "11.0.5"
  },
  "devDependencies": {
    "@deepdish/config": "workspace:0.2.2",
    "@tsconfig/create-react-app": "2.0.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "tsup": "8.2.4",
    "typescript": "5.5.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
