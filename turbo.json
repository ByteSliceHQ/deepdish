{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "//#code:check": {},
    "//#code:fix": {
      "cache": false
    },
    "build": {
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "dependsOn": ["^build"],
      "env": [
        "CLERK_SECRET_KEY",
        "BASE_URL",
        "DEEPDISH_CLOUD_ENDPOINT",
        "DEEPDISH_CLOUD_OAUTH_CLIENT_ID",
        "DEEPDISH_CLOUD_OAUTH_REDIRECT_URI",
        "DEEPDISH_CLOUD_STATE",
        "DEEPDISH_PROJECT_ALIAS",
        "DEEPDISH_SECRET_KEY",
        "DEEPDISH_MODE"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true,
      "env": [
        "CLERK_SECRET_KEY",
        "BASE_URL",
        "DEEPDISH_CLOUD_ENDPOINT",
        "DEEPDISH_CLOUD_OAUTH_CLIENT_ID",
        "DEEPDISH_CLOUD_OAUTH_REDIRECT_URI",
        "DEEPDISH_CLOUD_STATE",
        "DEEPDISH_PROJECT_ALIAS",
        "DEEPDISH_SECRET_KEY",
        "DEEPDISH_MODE"
      ]
    }
  }
}
