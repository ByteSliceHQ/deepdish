{
  "name": "@deepdish/ui",
  "version": "0.3.1",
  "description": "DeepDish UI components",
  "homepage": "",
  "author": {
    "name": "ByteSlice, LLC",
    "url": "https://byteslice.co"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepare": "bun run build"
  },
  "exports": {
    "./config": {
      "import": "./dist/config.mjs",
      "types": "./dist/config.d.ts",
      "require": "./dist/config.js"
    },
    "./link":{
      "import": "./dist/link.mjs",
      "types": "./dist/link.d.ts",
      "require": "./dist/link.js"
    },
    "./media": {
      "import": "./dist/media.mjs",
      "types": "./dist/media.d.ts",
      "require": "./dist/media.js"
    },
    "./typography": {
      "import": "./dist/typography.mjs",
      "types": "./dist/typography.d.ts",
      "require": "./dist/typography.js"
    }
  },
  "dependencies": {
    "@deepdish/resolvers": "*",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@tsconfig/create-react-app": "2.0.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "tsup": "8.2.4",
    "typescript": "5.4.5"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
