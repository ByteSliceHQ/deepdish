{
  "name": "@deepdish/resolvers",
  "version": "0.14.1",
  "description": "DeepDish content resolvers",
  "author": {
    "name": "ByteSlice, LLC",
    "url": "https://byteslice.co"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/resolver.js",
      "types": "./dist/resolver.d.ts"
    },
    "./json": {
      "import": "./dist/json.js",
      "types": "./dist/json.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@byteslice/result": "catalog:byteslice",
    "@deepdish/core": "workspace:0.6.0",
    "dataloader": "2.2.2",
    "valibot": "catalog:valibot"
  },
  "devDependencies": {
    "@types/bun": "catalog:repo",
    "tsup": "catalog:repo",
    "typescript": "catalog:repo"
  },
  "publishConfig": {
    "access": "public"
  }
}
