    "dev": "tsup --watch"
  },
  "dependencies": {
    "@byteslice/result": "0.1.0",
    "dataloader": "2.2.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/bun": "catalog:repo",
    "tsup": "catalog:repo",
    "typescript": "catalog:repo"
  },
  "publishConfig": {
    "access": "public"
  }
}
