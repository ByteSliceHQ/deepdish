{
  "name": "@deepdish/resolvers",
  "version": "0.4.0",
  "description": "DeepDish content resolvers",
  "homepage": "",
  "author": {
    "name": "ByteSlice, LLC",
    "url": "https://byteslice.co"
  },
  "exports": {
    ".": {
      "import": "./dist/resolver.mjs",
      "types": "./dist/resolver.d.ts",
      "require": "./dist/resolver.js"
    },
    "./json": {
      "import": "./dist/json.mjs",
      "types": "./dist/json.d.ts",
      "require": "./dist/json.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "tsup": "8.2.4",
    "typescript": "5.5.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
