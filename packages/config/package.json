{
  "name": "@deepdish/config",
  "version": "0.2.3",
  "description": "DeepDish configuration and schemas",
  "author": {
    "name": "ByteSlice, LLC",
    "url": "https://byteslice.co"
  },
  "exports": {
    ".": {
      "import": "./dist/config.mjs",
      "types": "./dist/config.d.ts",
      "require": "./dist/config.js"
    },
    "./schemas": {
      "import": "./dist/schemas.mjs",
      "types": "./dist/schemas.d.ts",
      "require": "./dist/schemas.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@byteslice/result": "0.1.0",
    "@logtape/logtape": "0.6.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@deepdish/resolvers": "workspace:0.4.1",
    "@tsconfig/node20": "20.1.4",
    "tsup": "8.2.4",
    "typescript": "5.5.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
